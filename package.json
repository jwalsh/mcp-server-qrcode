{
  "name": "@jwalsh/mcp-server-qrcode",
  "version": "0.2.0",
  "description": "Model Context Protocol server for generating QR codes",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "mcp-server-qrcode": "build/cli.js"
  },
  "files": [
    "build/",
    "README.org",
    "LICENSE",
    "CHANGELOG.org"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node build/cli.js",
    "dev": "ts-node --esm src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "ci": "npm run lint && npm run format:check && npm run typecheck && npm run test:ci",
    "prepublishOnly": "npm run build",
    "check-publish": "npm pack && tar -xvzf jwalsh-mcp-server-qrcode-*.tgz && ls -R package && rm -rf package && rm jwalsh-mcp-server-qrcode-*.tgz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.org -s",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major"
  },
  "keywords": [
    "mcp",
    "qrcode",
    "ai",
    "model-context-protocol"
  ],
  "author": "Jason Walsh <j@wal.sh> (https://wal.sh)",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "commander": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nyc": "^17.1.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
